{
  "name": "nest-finder-api-service",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server",
    "dev": "nodemon --legacy-watch server.js",
    "start:test": "NODE_ENV=test node src/app.js",
    "test": "jest --config jest.config.js --detectOpenHandles",
    "test:watch": "jest --config jest.config.js --watch",
    "test:coverage": "jest --config jest.config.js --coverage",
    "test:e2e": "start-server-and-test start:test http://localhost:8800 'jest --config jest-e2e.config.js'",
    "test:all": "yarn test && yarn test:e2e"
  },
  "repository": "git@github.com-codesmiles:codesmiles/nest-finder-api-service.git",
  "license": "MIT",
  "dependencies": {
    "cloudinary": "^2.5.1",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "express-fileupload": "^1.5.1",
    "express-rate-limit": "^7.4.1",
    "ioredis": "^5.4.1",
    "joi": "^17.13.3",
    "mysql": "^2.18.1",
    "mysql2": "^3.11.3",
    "nodemailer": "^6.9.16",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "sequelize": "^6.37.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.16.0"
  },
  "devDependencies": {
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "jest-puppeteer": "^10.1.4",
    "nodemon": "^3.1.7",
    "puppeteer": "^23.6.1",
    "start-server-and-test": "^2.0.8",
    "supertest": "^7.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true
  }
}
